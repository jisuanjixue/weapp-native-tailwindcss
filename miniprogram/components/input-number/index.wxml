<view class="wux-class {{ classes.wrap }}">
    <view
        bindlongpress="onLongpress"
        bindtap="onTap"
        bindtouchend="onTouchEnd"
        touchcancel="onTouchCancel"
        data-type="sub"
        class="wux-sub-class {{ classes.sub }}"
    >
     <van-icon name="minus" size="20px" custom-class="{{ classes.icon }}" />
    </view>
    <view class="wux-control-class {{ classes.control }}">
        <input
            bindinput="onInput"
            bindfocus="onFocus"
            bindblur="onBlur"
            value="{{ inputValue }}"
            disabled="{{ disabled || readOnly }}"
            type="digit"
            class="wux-input-class {{ classes.input }}"
        />
    </view>
    <view
        bindlongpress="onLongpress"
        bindtap="onTap"
        bindtouchend="onTouchEnd"
        touchcancel="onTouchCancel"
        data-type="add"
        class="wux-add-class {{ classes.add }}"
    >
    <van-icon name="plus" size="20px" custom-class="{{ classes.icon }}" />
    </view>
</view>