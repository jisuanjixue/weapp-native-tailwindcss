.mina-ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.mina-multi-ellipsis--l2 {
    -webkit-line-clamp: 2;
}
.mina-multi-ellipsis--l2,
.mina-multi-ellipsis--l3 {
    -webkit-box-orient: vertical;
    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
}
.mina-multi-ellipsis--l3 {
    -webkit-line-clamp: 3;
}
.mina-clearfix:after {
    clear: both;
    content: '';
    display: table;
}
.mina-hairline,
.mina-hairline--bottom,
.mina-hairline--left,
.mina-hairline--right,
.mina-hairline--surround,
.mina-hairline--top,
.mina-hairline--top-bottom {
    position: relative;
}
.mina-hairline--bottom:after,
.mina-hairline--left:after,
.mina-hairline--right:after,
.mina-hairline--surround:after,
.mina-hairline--top-bottom:after,
.mina-hairline--top:after,
.mina-hairline:after {
    border: 0 solid #ebedf0;
    bottom: -50%;
    box-sizing: border-box;
    content: ' ';
    left: -50%;
    pointer-events: none;
    position: absolute;
    right: -50%;
    top: -50%;
    transform: scale(0.5);
    transform-origin: center;
}
.mina-hairline--top:after {
    border-top-width: 1px;
}
.mina-hairline--left:after {
    border-left-width: 1px;
}
.mina-hairline--right:after {
    border-right-width: 1px;
}
.mina-hairline--bottom:after {
    border-bottom-width: 1px;
}
.mina-hairline--top-bottom:after {
    border-width: 1px 0;
}
.mina-hairline--surround:after {
    border-width: 1px;
}
